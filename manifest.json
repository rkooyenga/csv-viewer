{
  "short_name": "CSV Viewer",
  "name": "CSV/TSV Data Viewer",
  "icons": [
    {
      "src": "icon.svg",
      "type": "image/svg+xml",
      "sizes": "any"
    },
    {
        "src": "icon_192.png",
        "type": "image/png",
        "sizes": "192x192"
    },
    {
        "src": "icon_512.png",
        "type": "image/png",
        "sizes": "512x512"
    }
  ],
  "start_url": "index.html",
  "display": "standalone",
  "background_color": "#222",
  "theme_color": "#007bff",
  "share_target": {
      "action": "/index.html",
      "method": "GET",
      "params": {
          "title": "title",
          "text": "text",
          "url": "url",
          "files": [
              {
                  "name": "file",
                  "accept": ["text/csv", "text/tab-separated-values", ".csv", ".tsv", ".txt"]
              }
          ]
      }
  }
}
